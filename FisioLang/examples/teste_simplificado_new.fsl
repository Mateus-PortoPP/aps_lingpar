  y = 20;
 
  resultado(x + y);
  
  se melhorou(x > 5) {
    resultado(100);
  } caso contrario {
    resultado(200);
  }
  
  importar_dados("../examples/physical_therapy_exercises.csv");
 
  s = ler_sensor("amplitude");
  resultado(s);
  
